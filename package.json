{
  "version": "0.2.1-beta.4",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "homepage": "https://raid-guild.github.io/design-system",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "predeploy": "build-storybook",
    "deploy-storybook": "gh-pages -d storybook-static",
    "generate-icons": "node bin/generate-icons.js",
    "generate-icons:watch": "yarn generate-icons --watch"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@raidguild/design-system",
  "author": "Raid Guild",
  "module": "dist/design-system.esm.js",
  "size-limit": [
    {
      "path": "dist/design-system.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/design-system.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@rollup/plugin-replace": "^3.0.0",
    "@size-limit/preset-small-lib": "^5.0.3",
    "@storybook/addon-a11y": "^6.4.0",
    "@storybook/addon-actions": "6.4.0",
    "@storybook/addon-docs": "6.4.0",
    "@storybook/addon-links": "6.4.0",
    "@storybook/addon-viewport": "6.4.0",
    "@storybook/addons": "6.4.0",
    "@storybook/react": "6.4.0",
    "@svgr/core": "^5.5.0",
    "@svgr/plugin-jsx": "^5.5.0",
    "@svgr/plugin-prettier": "^5.5.0",
    "@svgr/plugin-svgo": "^5.5.0",
    "babel-loader": "^8.2.2",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.1",
    "prettier": "^2.5.0",
    "react": "^17.0.2",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "rollup-plugin-css-porter": "^1.0.2",
    "size-limit": "^5.0.3",
    "storybook-addon-react-docgen": "^1.2.42",
    "ts-loader": "8.1.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "4.2.2"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.6.6",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@types/react-dom": "^17.0.9",
    "framer-motion": "^4",
    "react-icons": "^4.3.1"
  }
}
